<div id="header">
  <ul id="primary">
  	<li>
  	<span>
  		Prompts List
		<ul id="secondary">
	  	  <li><%= link_to 'Create New prompt', new_fs_prompt_path %></li>
	    </ul>
	</span>
	</li>
  </ul>
</div>
<div id="main">
<div id="contents">

<h1>Prompt List</h1>

<table id="list">
  <tr>
	<th colspan=2>Actions</th><th>Name</th><th>Length</th><th>File</th><th>Description</th>
  </tr>

<% for fs_prompt in @fs_prompts %>
  <tr>
    <td><%= link_to 'Edit', edit_fs_prompt_path(fs_prompt) %></td>
    <td><%= link_to 'Destroy', fs_prompt, :confirm => 'Are you sure?', :method => :delete %></td>
	<td><%=fs_prompt.name%></td>
	<td><%=fs_prompt.length%>s</td>
	<td><%=fs_prompt.filename%></td>
	<td><%=fs_prompt.description%></td>
  </tr>
<% end %>
</table>

<br />

</div>
</div>

